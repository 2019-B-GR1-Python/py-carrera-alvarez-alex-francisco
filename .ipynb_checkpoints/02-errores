# errores de escritura
try:
   # a+b
    #0/0
    a={'b':'b'}
    a=['c']
except NameError as nameError:
    print("Name error")
except (ZeroDivisionError, keyError) as errorComun:
    print ("Error agrupado comun")
except Exception as error:
    print("Error")